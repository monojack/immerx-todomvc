{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard",
        "prettier",
        "prettier/react",
        "prettier/standard"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "chrome": "readonly"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "eqeqeq": "off",
        "no-unused-expressions": "off",
        "no-sequences": "off",
        "no-void": "off",
        "react/prop-types": "off",
        "react/no-unescaped-entities": "off",
        "camelcase": [
            "error",
            {
                "allow": [
                    "^Symbol_"
                ]
            }
        ]
    }
}
